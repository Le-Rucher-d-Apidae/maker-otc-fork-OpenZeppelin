{
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-foundry": "^1.0.1",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.9.2",
    "@primitivefi/hardhat-dodoc": "^0.2.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "chai": "^4.3.7",
    "concurrently": "^8.0.1",
    "del-cli": "^5.0.0",
    "dotenv": "^16.3.1",
    "ethers": "5.7.2",
    "hardhat": "2.13.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.34",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-tracer": "^2.4.0",
    "mocha": "^10.2.0",
    "solc": "^0.8.20",
    "solc-0.8": "npm:solc@^0.8.20",
    "solidity-coverage": "^0.8.2",
    "solidity-docgen": "0.6.0-beta.35",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "scripts": {
    "cleanHH": "npx hardhat clean",
    "cleanForge": "forge clean",
    "clean": "npm run cleanHH && npm run cleanForge",
    "dodoc": "npx hardhat dodoc",
    "docgen": "npx hardhat docgen",
    "docall": "concurrently \"npm run dodoc\" \"npm run docgen\"",
    "compile": "npx hardhat compile",
    "comp": "npm run compile",
    "console-no-compile": "npx hardhat console --no-compile",
    "console": "npx hardhat console",
    "cons": "npx hardhat console",
    "consoleLocalHost": "npx hardhat console --network localhost",
    "consLoc": "npx hardhat console --network localhost",
    "accounts": "npx hardhat accounts",
    "startLocalNode": "npx hardhat node",
    "node": "npx hardhat node --no-deploy",
    "nodeV": "npx hardhat node --no-deploy --verbose",
    "nodeD": "npx hardhat node",
    "nodeDV": "npx hardhat node --verbose",
    "testHH": "npx hardhat test",
    "testFoundryAll": "forge test -vv",
    "testFoundrySmpl": "forge test -vvv --match-contract ^SimpleMarket_*",
    "testFoundrySusp": "forge test -vvv --match-contract ^SuspendableSimpleMarket_*",
    "testFoundryRest1": "forge test -vvv --match-contract ^Restricted1SuspendableSimpleMarket_*",
    "testFoundryRest2": "forge test -vvv --match-contract ^Restricted2SuspendableSimpleMarket_*",
    "testFoundryRestMM1": "forge test -vvv --match-contract ^RestrictedSuspendableMatchingMarket1_*",
    "testFoundryRestMM2": "forge test -vvv --match-contract ^RestrictedSuspendableMatchingMarket2_*",
    "testAll": "npm run testHH && npm run testFoundry",
    "testPG": "npx hardhat test --network localhost --grep PayableGreeter",
    "testTrace": "npx hardhat test --trace",
    "testFulltrace": "npx hardhat test --fulltrace",

    "HhDeployLoc": "npx hardhat --network localhost deploy",
    "depUniOraLocV": "npx hardhat run scripts/deployUniswapV3Oracle.js --network localhost --verbose",
    "depGLoc": "npx hardhat run scripts/deployGreeter.js --network localhost",
    "depPGLoc": "npx hardhat run scripts/deployPayableGreeter.js --network localhost",
    "depLocV": "npx hardhat run scripts/deployGreeter.js --network localhost --verbose",

    "depTokenLoc": "npx hardhat deploy --tags Token --network localhost",
    "depOracleLoc": "npx hardhat deploy --tags Oracle --network localhost",
    "depMatchingMartketLoc": "npx hardhat deploy --tags MatchingMarket --network localhost",
    "depOracleMumb": "npx hardhat deploy --tags Oracle --network mumbai",
    "depUniOraMumb2": "npx hardhat run scripts/deployUniswapV3Oracle.js --network mumbai --verbose",
    "depMatchingMartketMumb": "npx hardhat deploy --tags MatchingMarket --network mumbai",
    "verifyMumb": "npx hardhat etherscan-verify --network mumbai",


    "sourcifyMumb": "npx hardhat --network mumbai mainnet sourcify",
    

    "installNCU": "npm i -g npm-check-updates",
    "updatePackages": "ncu --upgrade",
    "updateNpm": "npm update -g",
    "npm:cleanCache": "npm cache clean -f",
    "cleanBuildDir": "del-cli ./artifacts",
    "cleanDocsDir": "del-cli ./docs/generated/solidity-docs/natspec/",
    "clnBld": "npm run cleanBuildDir",
    "size": "npx hardhat size-contracts --no-compile"
  }
}
